public class Lesson6Task2 {
    public static void main(String[] args) {
        int[]arr={1,8,3,11,5,6};
        int max=arr[0];
        for(int i=0;i<arr.length;i++){
            if(arr[i]>max){
                max=arr[i];
                break;
            }
        }

        System.out.println(max);
    }
}
