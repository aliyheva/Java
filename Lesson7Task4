public class Lesson7Task4 {
    public static void main(String[] args){
        String s="Hello World";
        int l=s.length();
        int count=0;
        for(int i=0;i<l;i++){
            int symbol=s.charAt(i);
            if(symbol !='A'&& symbol !='a'){
             count++;
            }
        }
        System.out.println(count);
    }
}
