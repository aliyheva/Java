public class Lesson5Task7 {
    public static void main(String[] args) {
        int k=15;
        for(int i=1;i<=9;i++){
            for(int z=0;z<k;z++){
                System.out.print(" ");
            }
            for(int j=1;j<=i;j++){
                System.out.print(i + " ");
            }
            System.out.println();
            k--;
        }
    }
}
