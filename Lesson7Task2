public class Lesson7Task2 {
    public static void main(String[] args) {
        String sen1="Hello World";
        String sen2="Hi World";
        int len1=sen1.length();
        int len2=sen2.length();
        int mod=Math.abs(len1-len2);
        if(len1>len2){
            System.out.println(Math.pow(len1,mod));
        }
    }
}
